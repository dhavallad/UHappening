<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:UHappening"
             x:Class="UHappening.HomPage"
             Title="UHappening">
  <StackLayout Orientation="Vertical" VerticalOptions="Center">
    <StackLayout Orientation="Vertical" VerticalOptions="Center" HorizontalOptions="Center" WidthRequest="200">
      <Button Text="Create Event" Clicked="createEventClicked"></Button>
      <Button Text="My Events" Clicked="myEventsClicked"></Button>
      <Button Text="Private Events" Clicked="myEventsClicked"></Button>
      <Button Text="Public Events" Clicked="myEventsClicked"></Button>
    </StackLayout>
    <!--<StackLayout VerticalOptions="End" Padding="50" WidthRequest="200">
      <Button Text="Log Out"></Button>
    </StackLayout>-->
  </StackLayout>
</ContentPage>